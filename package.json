{
  "name": "rsql-builder",
  "version": "1.3.3",
  "description": "RSQL query builder",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint",
    "lint:fix": "npm run lint -- --fix",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "prepush": "npm test",
    "pretest": "npm run build",
    "start": "ts-node --files -P ./tsconfig.json",
    "test": "c8 --reporter=text --reporter=lcov --include=dist node --test tests/*.spec.js",
    "test:no-coverage": "node --test tests/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RomiC/rsql-builder.git"
  },
  "keywords": [
    "rsql",
    "query",
    "builder"
  ],
  "engines": {
    "node": ">= 20.0.0"
  },
  "author": "Roman Charugin <i@charugin.ru>",
  "contributors": [
    "Roman Charugin <i@charugin.ru>",
    "Mihael Šafarić <mihael.safaric@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RomiC/rsql-builder/issues"
  },
  "homepage": "https://github.com/RomiC/rsql-builder#readme",
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@types/node": "25.0.3",
    "c8": "^10.1.3",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsdoc": "62.5.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "5.5.5",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "prettier": "3.8.1",
    "tsup": "8.5.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.54.0"
  },
  "lint-staged": {
    "*.ts": "npm run lint:fix"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false
}
