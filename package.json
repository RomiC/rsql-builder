{
  "name": "rsql-builder",
  "version": "1.1.0",
  "description": "RSQL query builder",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p .",
    "lint": "eslint README.md src tests",
    "lint:fix": "npm run lint -- --fix",
    "precommit": "lint-staged",
    "prepush": "npm test",
    "start": "ts-node --files -P ./tsconfig.json",
    "test": "jest",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RomiC/rsql-builder.git"
  },
  "keywords": [
    "rsql",
    "query",
    "builder"
  ],
  "engines": {
    "node": ">= 12.0.0"
  },
  "author": "Roman Charugin <i@charugin.ru>",
  "contributors": [
    "Roman Charugin <i@charugin.ru>",
    "Mihael Šafarić <mihael.safaric@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RomiC/rsql-builder/issues"
  },
  "homepage": "https://github.com/RomiC/rsql-builder#readme",
  "devDependencies": {
    "@types/jest": "27.5.1",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/eslint-plugin-tslint": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "eslint": "8.16.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsdoc": "37.9.7",
    "eslint-plugin-markdown": "2.2.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.4.2",
    "prettier": "2.5.1",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5"
  },
  "lint-staged": {
    "*.ts": "npm run lint:fix"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/*.ts"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/tests/"
    ]
  }
}
