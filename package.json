{
  "name": "rsql-builder",
  "version": "1.0.3",
  "description": "RSQL query builder",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p .",
    "lint": "tslint -c tslint.json '{src,tests}/*.ts'",
    "lint:fix": "npm run lint -- --fix",
    "precommit": "lint-staged",
    "prepush": "npm test",
    "start": "ts-node --files -P ./tsconfig.json",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RomiC/rsql-builder.git"
  },
  "keywords": [
    "rsql",
    "query",
    "builder"
  ],
  "engines": {
    "node": ">= 10.0.0"
  },
  "author": "Roman Charugin <i@charugin.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RomiC/rsql-builder/issues"
  },
  "homepage": "https://github.com/RomiC/rsql-builder#readme",
  "devDependencies": {
    "@types/jest": "25.2.3",
    "coveralls": "^3.1.0",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "lint-staged": "10.2.4",
    "ts-jest": "26.0.0",
    "tslint": "6.1.2",
    "tslint-eslint-rules": "5.4.0",
    "typescript": "3.9.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint:fix",
      "git add"
    ]
  }
}
